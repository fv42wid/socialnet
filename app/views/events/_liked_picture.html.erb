<li class="media">
  <h5><%= event.user.email %> <small>liked this</small></h5>
  <div class="media-left">
    <%= image_tag 'http://placehold.it/50x50', :class => "media-object" %>
  </div> <!-- media-left -->
  <div class="media-body">
    <h5 class="post-name-heading"><%= event.eventable.user.email %></h5>
    <p class="text-muted post-time-ago">
      <small><%= time_ago_in_words(event.eventable.updated_at) %></small>
    </p>
    <%= image_tag event.eventable.location, :class => 'img-responsive' %>
    <div id="picture_<%= event.eventable_id %>_likes" class="text-left picture_likes">
      <%= render partial: 'pictures/likes', locals: {picture: event.eventable} %>
    </div>
    
  </div>
</li>
